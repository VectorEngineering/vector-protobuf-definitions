    "glob": "^10.3.10",
    "handlebars": "^4.7.8",
    "hono": "^4.6.20",
    "openapi-types": "^12.1.3",
    "openapi-zod-client": "^1.18.2",
    "ora": "^7.0.1",
    "yaml": "^2.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@cloudflare/workers-types": "^4.20250129.0",
